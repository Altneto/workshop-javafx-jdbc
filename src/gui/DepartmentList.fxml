         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <padding>
            <Insets left="5.0" top="5.0" />
         </padding></Label>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button fx:id="btNew" mnemonicParsing="false" onAction="#onBtNewAction" text="Novo" />
        </items>
      </ToolBar>
      <TableView fx:id="tableViewDepartment" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="tableColumnId" prefWidth="44.799986243247986" text="Id" />
          <TableColumn fx:id="tableColumnName" prefWidth="105.60004425048828" text="Nome" />
            <TableColumn fx:id="tableColumnEDIT" prefWidth="45.60003662109375" />
            <TableColumn fx:id="tableColumnREMOVE" prefWidth="51.20001220703125" />
        </columns>
      </TableView>
   </children>
